#login,page{background-color:#fff}#login{font-size:16rpx;padding:0 15rpx;margin-top:50rpx}#login .tips{position:fixed;top:0;left:0;right:0;text-align:center;line-height:35rpx;background-color:#e64340;height:35rpx;color:#FFF}#login .register_header{padding:36rpx 0 56rpx;text-align:center;color:#10dbd4;font-size:30rpx}#login .register_input .code .writecode .input-placeholder,#login .register_input .tellphone .input-placeholder{color:#9B9B9B;font-size:14rpx}#login .register_input{margin-bottom:60rpx}#login .register_input .tellphone{background:rgba(155,155,155,.3);margin-bottom:20rpx;border-radius:4rpx}#login .register_input .tellphone input{padding:15rpx 0 15rpx 15rpx;border-radius:4rpx}#login .register_input .code{display:flex;align-items:center;justify-content:space-between}#login .register_input .code .writecode{width:60%;background:rgba(155,155,155,.3);border-radius:4rpx}#login .register_input .code .writecode input{padding:15rpx 0 15rpx 15rpx}#login .register_input .code .getCode{padding:15rpx 0;flex:1;margin-left:17rpx;background-color:#EB7073;text-align:center;color:#FFF;border-radius:4rpx;font-size:14rpx}#login .select{display:flex;align-items:center;justify-content:center;margin-top:15rpx;color:#4A4A4A;font-size:12rpx}#login .select checkbox .wx-checkbox-input{background-color:#fff;border:1rpx solid #c1caca;box-shadow:0 1rpx 2rpx rgba(0,0,0,.05),inset 0 -15rpx 10rpx -12rpx rgba(0,0,0,.05);height:14rpx;width:14rpx}#login .select checkbox .wx-checkbox-input.wx-checkbox-input-checked{background-color:#10dbd4;border:1rpx solid #10dbd4}#login .submit button{color:#fff;padding:10rpx 0;border:none;border-radius:4rpx;background-color:#C9C8C8;font-size:16rpx}#login .submit button.check{background:#10dbd4}