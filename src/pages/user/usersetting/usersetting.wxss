#user-setting view{overflow:visible}#user-setting .headImg{position:relative;width:90rpx;height:90rpx;border:1rpx solid #8d8d8d;border-radius:100%;display:block;margin:20rpx auto}#user-setting .headImg:after{position:absolute;content:"";background:url(https://ibrand-miniprogram.oss-cn-hangzhou.aliyuncs.com/小程序/ic_camer.png) no-repeat;width:23rpx;height:23rpx;bottom:0;right:0;background-size:100% 100%}#user-setting .headImg image{width:88rpx;height:88rpx;border-radius:50%}#user-setting .user_setting{background-color:#fff}#user-setting .user_setting .code{border-bottom:1rpx solid #e5e5e5;padding:10rpx 0;display:flex;align-items:center;font-size:14rpx}#user-setting .user_setting .code .detail{padding-left:10rpx;width:80rpx;color:#a4a3a3}#user-setting .user_setting .code input{flex:1;color:#000}#user-setting .user_setting .code picker{flex:1;position:relative}#user-setting .user_setting .code .under{position:absolute;transform:rotate(90deg);right:15rpx;top:2rpx;font-size:12rpx;color:#a4a3a3}#user-setting .user_setting .code span{padding:0 15rpx;color:#00b4ff}#user-setting .submit{width:100%;position:fixed;bottom:0}#user-setting .submit button[type=primary]{color:#FFF;background-color:#10dbd4;border-radius:0}